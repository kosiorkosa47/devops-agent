{
  "name": "devops-agent-cms",
  "version": "1.0.0",
  "description": "Payload CMS for DevOps Agent",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "npm run build:payload && npm run build:server",
    "start": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types"
  },
  "dependencies": {
    "payload": "^2.8.0",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "@payloadcms/db-postgres": "^0.8.0",
    "@payloadcms/richtext-slate": "^1.5.0",
    "@payloadcms/plugin-cloud-storage": "^1.1.0",
    "@aws-sdk/client-s3": "^3.485.0",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
